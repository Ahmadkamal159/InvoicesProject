@model InvoicesProject.Models.NWC_Rreal_Estate_Type



<h1>انشاء وحدة</h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="All" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="NWC_Rreal_Estate_TypeID" class="control-label"></label>
                <input asp-for="NWC_Rreal_Estate_TypeID" class="form-control" />
                <span asp-validation-for="NWC_Rreal_Estate_TypeID" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NWC_Rreal_Estate_Type_Name" class="control-label"></label>
                <input asp-for="NWC_Rreal_Estate_Type_Name" class="form-control" />
                <span asp-validation-for="NWC_Rreal_Estate_Type_Name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NWC_Rreal_Estate_Type_Reasons" class="control-label"></label>
                <input asp-for="NWC_Rreal_Estate_Type_Reasons" class="form-control" />
                <span asp-validation-for="NWC_Rreal_Estate_Type_Reasons" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="حفظ" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a class="btn btn-primary" asp-action="Index">قائمة الوحدات</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
